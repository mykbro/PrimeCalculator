<Window x:Class="PrimeCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:loc="clr-namespace:PrimeCalculator"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"      
        mc:Ignorable="d"
        Title="Prime Calculator" Background="Azure" MinHeight="250" MinWidth="150" SizeToContent="WidthAndHeight" Loaded="Window_Loaded">
    <Window.Resources>
        <loc:PrimeCalc x:Key="defaultControl" CalcButtonClick="PrimeCalc_CalcButtonClick" StopButtonClick="PrimeCalc_StopButtonClick" SwitchLangButtonClick="PrimeCalc_SwitchLangButtonClick">            
        </loc:PrimeCalc>
    </Window.Resources>    

</Window>

